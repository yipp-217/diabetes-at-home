    <!--unique id-->
    <canvas id={{chartType}}></canvas>

     <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <script>
    //unique const name
    const thisChart = new Chart(
      document.getElementById({{chartType}}),
      {
        type: 'line',
        data: {
        //import date here
        labels: {{xAxesData}},
        datasets: [{
        backgroundColor: 'black',
        borderColor: 'black',
        //import health data value here
         data: {{yAxesDate}},
        }]},
        options: {
          scales: {
            yAxes: {
                title: {
                    display: true,
                    text: "hiiii",
                },
                ticks: {
                  min: 50,
                  max: 500
                }
            },
            xAxes: {
                title: {
                    display: true,
                    text: "Date of recording data",
                }
            }
        },
          plugins: {legend: {display: false}}}
    }
    );
    </script>